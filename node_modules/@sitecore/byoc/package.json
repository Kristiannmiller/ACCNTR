{
  "type": "module",
  "name": "@sitecore/byoc",
  "description": "Bring-Your-Own-Components runtime to register and retrieve react components",
  "version": "0.2.18",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sitecore-PD/sitecore.cloudsdk.byoc.git"
  },
  "scripts": {
    "test": "npx vitest",
    "types": "tsc -p tsconfig.json",
    "build:esbuild": "node scripts/build.js",
    "build": "npm run types && npm run build:esbuild",
    "prepublishOnly": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@homer0/prettier-plugin-jsdoc": "^8.0.0",
    "@types/jest": "^29.5.6",
    "@types/json-schema": "^7.0.13",
    "@types/react": "^18.2.25",
    "@vitest/coverage-v8": "^1.2.1",
    "esbuild": "^0.19.4",
    "glob": "^10.3.10",
    "jsdom": "^22.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  },
  "main": "./dist/node/index.cjs",
  "browser": "./dist/browser/index.cjs",
  "module": "./dist/node/index.esm.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "localhost": "./build/index.js",
      "types": "./types/index.d.ts",
      "browser": "./dist/browser/index.esm.js",
      "import": "./dist/node/index.esm.js",
      "require": "./dist/node/index.cjs",
      "default": "./dist/node/index.esm.js"
    },
    "./react": {
      "localhost": "./build/react.js",
      "types": "./types/react.d.ts",
      "browser": "./dist/browser/react.esm.js",
      "import": "./dist/node/react.esm.js",
      "require": "./dist/node/react.cjs",
      "default": "./dist/node/react.esm.js"
    }
  },
  "dependencies": {
    "@rjsf/utils": "*",
    "json-schema": "^0.4.0"
  },
  "directories": {
    "test": "test"
  }
}
