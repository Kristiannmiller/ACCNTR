{
    "name": "@ahc-digital/component-library",
    "version": "1.0.0",
    "type": "module",
    "repository": "https://github.com/ahc-digital/component-library",
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    },
    "main": "dist/index.umd.js",
    "module": "dist/index.es.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.js",
            "types": "./dist/index.d.ts"
        },
        "./package.json": "./package.json",
        "./dist/*": "./dist/*"
    },
    "files": [
        "/dist"
    ],
    "scripts": {
        "build": "tsc && vite build",
        "storybook": "storybook dev -p 6006 --no-open",
        "sb": "storybook dev -p 6006 --no-open",
        "dev": "storybook dev -p 6006 --no-open",
        "build-storybook": "storybook build",
        "lint": "stylelint --allow-empty-input \"**/*.scss\"",
        "format": "prettier --write .",
        "generate": "plop",
        "lint:js": "eslint --ext \".ts,.tsx\" --ignore-path vite.config.ts",
        "lint:unused-imports": "eslint . --ext .ts,.tsx",
        "lint:all": "npm run lint && npm run lint:js && npm run lint:unused-imports",
        "lint-staged": "lint-staged",
        "test": "jest",
        "test:coverage": "npm test -- --coverage",
        "prepare": "husky",
        "test-storybook": "test-storybook",
        "deploy-local-search": "npm run build && npm pack --pack-destination ../Search-Solution",
        "deploy-local-sitecore": "npm run build && npm pack --pack-destination ../sitecore-solution"
    },
    "dependencies": {
        "@radix-ui/react-accordion": "^1.1.2",
        "@radix-ui/react-collapsible": "^1.0.3",
        "@radix-ui/react-navigation-menu": "^1.1.4",
        "@radix-ui/react-select": "^2.1.1",
        "@radix-ui/react-slot": "^1.0.2",
        "@radix-ui/themes": "^2.0.3",
        "class-variance-authority": "0.7.0",
        "esbuild": "^0.14.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-scrollspy-nav": "^1.3.0",
        "swiper": "^11.1.1"
    },
    "devDependencies": {
        "@awesome.me/kit-6f3f1f8c8b": "^1.0.39",
        "@fortawesome/fontawesome-svg-core": "^6.5.2",
        "@fortawesome/react-fontawesome": "^0.2.0",
        "@sitecore-jss/sitecore-jss-react": "^21.6.4",
        "@storybook/addon-a11y": "^7.6.13",
        "@storybook/addon-coverage": "^1.0.4",
        "@storybook/addon-designs": "^7.0.9",
        "@storybook/addon-essentials": "^7.6.17",
        "@storybook/addon-interactions": "^7.6.17",
        "@storybook/addon-links": "^7.6.17",
        "@storybook/addons": "^7.6.17",
        "@storybook/blocks": "^7.6.17",
        "@storybook/jest": "^0.2.3",
        "@storybook/nextjs": "^7.6.17",
        "@storybook/react": "^7.6.17",
        "@storybook/react-vite": "^7.6.17",
        "@storybook/test": "^7.6.17",
        "@storybook/test-runner": "^0.19.1",
        "@storybook/testing-library": "^0.2.2",
        "@storybook/theming": "^7.6.17",
        "@types/css-modules": "^1.0.5",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.11.1",
        "@types/react": "^18.2.64",
        "@types/react-dom": "^18.2.21",
        "@typescript-eslint/eslint-plugin": "^7.1.1",
        "@typescript-eslint/parser": "^7.1.1",
        "@vitejs/plugin-react": "^4.2.1",
        "autoprefixer": "^10.4.19",
        "conventional-changelog-conventionalcommits": "^7.0.2",
        "eslint": "^8.57.0",
        "eslint-config-next": "14.1.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "eslint-plugin-storybook": "^0.8.0",
        "eslint-plugin-unused-imports": "^3.2.0",
        "husky": "^9.0.11",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.2",
        "plop": "^4.0.1",
        "postcss": "^8.4.38",
        "prettier": "2.8.8",
        "react-syntax-highlighter": "15.5.0",
        "sass": "^1.71.1",
        "semantic-release": "^23.0.2",
        "storybook": "^7.6.17",
        "stylelint": "^16.6.1",
        "stylelint-config-prettier-scss": "^1.0.0",
        "stylelint-config-standard-scss": "^13.1.0",
        "ts-jest": "^29.1.2",
        "typescript": "^5.2.2",
        "typescript-eslint": "^7.2.0",
        "vite": "^5.1.6",
        "vite-plugin-dts": "3.7.2",
        "vite-tsconfig-paths": "^4.3.1"
    },
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-gnu": "^4.13.0"
    },
    "packageManager": "yarn@1.22.19+sha512.ff4579ab459bb25aa7c0ff75b62acebe576f6084b36aa842971cf250a5d8c6cd3bc9420b22ce63c7f93a0857bc6ef29291db39c3e7a23aab5adfd5a4dd6c5d71"
}
